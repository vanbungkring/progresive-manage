<div class="bg-light lter b-b wrapper-md">
    <h1 class="m-n font-thin h3"><%=title%></h1>
</div>
<div class="wrapper-md">
    <div class="col-sm-8">

        <div class="panel panel-default">
            <div class="panel-body">
                <form role="form" method="POST" ui-jq="parsley">
                    <div class="row">
                        <div class="form-group col-md-6">
                            <label>Nama Lengkap</label>
                            <input type="text" class="form-control" required name="name" value="<%= typeof data.data!='undefined' && data.data.name ? data.data.name : '' %>">
                        </div>
                    </div>

                        <!-- <div class="form-group col-md-6">
                            <label>Jenis Kelamins</label>
                            <input type="text" class="form-control" required name="gender" value="<%= data.gender ? data.gender : '' %>">
                        </div> -->
                    <div class="row">
                        <div class="form-group col-md-6">
                            <label>Email</label>
                            <input type="email" class="form-control" required name="email" value="<%= typeof data.data!='undefined' && data.data.username ? data.data.username : '' %>">
                        </div>
                    </div>
                        <!-- <div class="form-group col-md-6">
                            <label>Tanggal Lahir</label>
                            <input type="text" class="form-control" required name="dob" value="<%= data.name ? data.name : '' %>">
                        </div> -->
                    <div class="row">
                        <div class="form-group col-md-6">
                            <label>Telepon</label>
                            <input type="text" class="form-control" name="phoneNumber" value="<%= typeof data.data!='undefined' && data.data.phoneNumber ? data.data.phoneNumber : '' %>">
                        </div>
                    </div>
                    <div class="form-group">
                        <label>Alamat</label>
                        <textarea class="form-control" rows="3" name="address"><%= typeof data.data!='undefined' && data.data.address.address ? data.data.address.address : '' %></textarea>
                    </div>

                    <div class="line line-dashed b-b line-lg pull-in"></div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-success">Simpan</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
