<div class="staff-picked-posts padding-top-30 background-color-gray padding-bottom-40" id="chennal-page">
    <div class="container">
        <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                <li data-target="#carousel-example-generic" data-slide-to="1"></li>
                <li data-target="#carousel-example-generic" data-slide-to="2"></li>
                  <li data-target="#carousel-example-generic" data-slide-to="3"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner">
                <div class="item active">
                    <img src="http://res.cloudinary.com/oddro/image/upload/v1495946032/banner/benner1.png" alt="...">
                </div>
                <div class="item">
                    <img src="http://res.cloudinary.com/oddro/image/upload/v1495946032/banner/benner2.png" alt="...">
                </div>
                <div class="item">
                    <img src="http://res.cloudinary.com/oddro/image/upload/v1495946032/banner/benner3.png" alt="...">
                </div>
                <div class="item">
                    <img src="http://res.cloudinary.com/oddro/image/upload/v1495946032/banner/benner4.png" alt="...">
                </div>
            </div>

            <!-- Controls -->
            <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left"></span>
            </a>
            <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right"></span>
            </a>
        </div>
        <!-- Carousel -->
        <div class="tab-content">
            <div class="blog-page">
                <div class="video-grid__container">
                    <h2 class="video-grid__title">VIDEO PILIHAN</h2></div>
                <div class="carousel-inner">
                    <div class="item active">
                        <div class="row">
                            <ul class="staff-picked-videos">
                                <% data.data.forEach(function(campaign){ %>

                                    <li class="col-md-3 col-sm-3 col-xs-12">
                                        <div class="post-details">
                                            <div class="overlay-inner-image">
                                                <img src=<%=youtubeThumbnail(campaign.postMeta.videoUrl)%> alt=">
                                                <%=campaign.postMeta.title%>">
                                                    <a href="/content/video/<%=campaign.postMeta.slug%>" class="image-overlay"></a>


                                            </div>
                                            <div class="image-content background-color-white">
                                                <p class="title-main">
                                                    <a href="/content/video/<%=campaign.postMeta.slug%>">
                                                        <%=excerpt(campaign.postMeta.title,30)%>
                                                    </a>
                                                </p>
                                                <p class="margin-bottom-0">
                                                    <%=campaign.author.name%>
                                                </p>
                                                <div class="banner-category">
                                                    <a href="#" class="cat">
                                                        <%=campaign.campaignCategory.name%>
                                                    </a>
                                                </div>

                                            </div>
                                        </div>
                                    </li>
                                    <%});%>


                            </ul>
                        </div>
                        <!-- /Slide1 -->
                    </div>
                    <!-- /Slide1 -->

                </div>
            </div>
            <!-- /#myCarousel -->
        </div>

    </div>


</div>
